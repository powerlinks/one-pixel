{
  "name": "powerlinks/one-pixel",
  "type": "library",
  "description": "Faster way to get a transparent pixel for tracking purpose",
  "keywords": ["powerlinks", "one-pixel", "gif"],
  "homepage": "http://www.powerlinks.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "Manuel Kanah",
      "email": "manuel@powerlinks.com",
      "homepage": "http://www.powerlinks.com/",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.5.*@dev"
  },
  "autoload": {
    "psr-4": {
      "PowerLinks\\OnePixel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PowerLinks\\OnePixel\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}